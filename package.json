{
  "name": "figma-ui-components",
  "version": "0.0.4",
  "description": "Unofficial set of Figma UI components",
  "main": "./dist/index.js",
  "scripts": {
    "flow": "flow",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "prettier": "prettier \"*/**/*.tsx\" \"*/**/*.ts\" --ignore-path ./.prettierignore --write && git add . && git status",
    "build": "npm run build:clean && npm run build:lib",
    "build:clean": "rimraf dist",
    "build:lib": "cross-env BABEL_ENV=production tsc -p tsconfig.json --outDir dist"
  },
  "pre-commit": [
    "prettier"
  ],
  "author": "Ilya Lesik <ilialesik@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "7.0.0",
    "@storybook/addon-actions": "5.0.10",
    "@storybook/addon-info": "5.0.10",
    "@storybook/addon-knobs": "5.0.10",
    "@storybook/react": "5.0.10",
    "@types/jest": "^24.0.16",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.18",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "8.0.5",
    "cross-env": "^5.2.0",
    "flow-bin": "^0.101.1",
    "gen-flow-files": "^0.4.6",
    "jest": "^24.8.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.17.0",
    "react-docgen-typescript-loader": "^3.1.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@types/storybook__addon-knobs": "^5.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-primitives": "^0.8.0",
    "react-test-renderer": "^16.8.6",
    "styled-components": "^4.3.2"
  }
}
